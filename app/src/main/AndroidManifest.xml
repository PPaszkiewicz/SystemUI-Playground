<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.ppaszkiewicz.kotlin.systemuiplayground">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SystemUIPlayground">
        <activity
            android:name=".SettingsActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".SettingsActivity30"
            android:label="@string/controller_playground" />
        <activity android:name=".ChoiceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AnimActivity" />
    </application>

</manifest>